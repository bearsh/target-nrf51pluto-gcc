{
  "name": "nrf51pluto-gcc",
  "version": "1.1.0",
  "inherits": {
    "nordic-nrf51822-gcc": "^1.0.0"
  },
  "description": "Unofficial mbed build target for the nrf51 Pluto 16KB platform.",
  "homepage": "https://github.com/bearsh/target-nrf51pluto-gcc",
  "repository": {
    "url": "git@github.com:bearsh/target-nrf51pluto-gcc.git",
    "type": "git"
  },
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "pluto",
    "mbed-target:nrf51_pluto",
    "ble",
    "nrf51822",
    "gcc"
  ],
  "similarTo": [
    "nrf51dk"
  ],
  "config": {
    "nrf51822": {
      "ram_size": "16K"
    },
    "image": {
      "heap": {
        "warning_threshold": 1024
      }
    },
    "hardware": {
      "clocks": {
        "nrf": {
          "lfclk_src": 0
        }
      },
      "pins": {
        "LED1": "NC",
        "LED2": "NC",
        "LED3": "NC",
        "LED4": "NC",
        "RX_PIN_NUMBER": "NC",
        "TX_PIN_NUMBER": "NC",
        "CTS_PIN_NUMBER": "NC",
        "RTS_PIN_NUMBER": "NC",
        "KS100_TX": "p6",
        "KS100_RX": "p5"
      }
    }
  }
}
